<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE tiles-definitions PUBLIC  
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"  
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">  
<tiles-definitions>  
 
    <!-- Base Define -->
    <definition name="base.definition" 
        template="/WEB-INF/view/static/layout.jsp">  
        <put-attribute name="title" value="" />  
        <put-attribute name="header" value="/WEB-INF/view/static/header.jsp" />  
        <put-attribute name="menu" value="/WEB-INF/view/static/menu.jsp" />  
        <put-attribute name="body" value="" />  
        <put-attribute name="footer" value="/WEB-INF/view/static/footer.jsp" />  
    </definition>  
 
    <!-- Home Page -->
    <definition name="homePage" extends="base.definition">  
        <put-attribute name="title" value="Home Page" />  
        <put-attribute name="body" value="/WEB-INF/view/custom/home.jsp" />  
    </definition>  
       
    <!-- ContactUs Page -->
    <definition name="contactusPage" extends="base.definition">  
        <put-attribute name="title" value="Contact Us" />  
        <put-attribute name="body" value="/WEB-INF/view/custom/contactus.jsp" />  
    </definition>  
  
 
  
</tiles-definitions> 